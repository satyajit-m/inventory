<p id="notice"><%= notice %></p>

<h1>Storages</h1>

<table>
  <thead>
    <tr>
      <th>Item</th>
      <th>Qty</th>
      <th>Procure time</th>
      <th>Buffer</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @storages.each do |storage| %>
      <tr>
        <td><%= storage.item_id %></td>
        <td><%= storage.qty %></td>
        <td><%= storage.procure_time %></td>
        <td><%= storage.buffer %></td>
        <td><%= link_to 'Show', storage %></td>
        <td><%= link_to 'Edit', edit_storage_path(storage) %></td>
        <td><%= link_to 'Destroy', storage, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Storage', new_storage_path %>
